PROJECT_NAME           = "@PROJECT_NAME@"
PROJECT_NUMBER         = "@PROJECT_VERSION@"
PROJECT_BRIEF          = "MQTT Client Library"
OUTPUT_DIRECTORY       = "@CMAKE_CURRENT_BINARY_DIR@/docs"
INPUT                  = "@CMAKE_CURRENT_SOURCE_DIR@/include/public"
RECURSIVE              = YES
EXTRACT_ALL            = NO
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = NO
HIDE_UNDOC_MEMBERS     = YES
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
USE_MDFILE_AS_MAINPAGE = @CMAKE_CURRENT_SOURCE_DIR@/README.md
FILE_PATTERNS          = *.h *.hpp
EXCLUDE_PATTERNS       = */private/* */internal/*
JAVADOC_AUTOBRIEF      = YES
QT_AUTOBRIEF           = YES
WARNINGS               = YES
WARN_IF_UNDOCUMENTED   = YES